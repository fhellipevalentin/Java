package Exercicio;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		int d = sc.nextInt();
		
		if (a < b && a < c && a < d) {
			System.out.printf("MENOR =" + a);
		} 
		else if (b < c && b < d ) {
				System.out.println("MENOR = " + b);
			}
		else if (c < d) {
				System.out.println("MENOR =" + c);
			}
		else {
				System.out.println("MENOR =" + d);
			}
		sc.close();		
		}
}	
			


	
