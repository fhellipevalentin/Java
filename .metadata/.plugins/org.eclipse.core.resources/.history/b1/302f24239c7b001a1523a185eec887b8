package Exercicio;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		int y = sc.nextInt();
		
		while (x < y) {
			System.out.println("Crescente");			
		}
		while (y > x) {
			System.out.println("Decrescente");
		}
		
		sc.close();	
		
	}
}
	
			


	
