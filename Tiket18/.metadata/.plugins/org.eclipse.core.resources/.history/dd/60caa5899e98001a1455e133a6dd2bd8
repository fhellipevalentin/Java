package application;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Digite três alturas: ");
		for (int i=1; i<=3; i++) {
			double altura = sc.nextDouble();
			if (altura < 1.80) {
				System.out.printf("%d%n: Altura permitida",i);
			}
			else {
				System.out.printf("%d%n: Altura excede o permitido",i);
			}
		}
		
		
		
		
		sc.close();

	}

}
